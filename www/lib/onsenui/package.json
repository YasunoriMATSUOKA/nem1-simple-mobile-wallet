{
  "name": "onsenui",
  "version": "2.10.8",
  "description": "HTML5 Mobile Framework & UI Components",
  "private": false,
  "scripts": {
    "build": "(cd bindings/angular1 && yarn install) && gulp build",
    "dev": "gulp serve --css --core",
    "test": "gulp test --disable-warnings",
    "test:skip-build": "gulp test --disable-warnings --skip-build"
  },
  "main": "js/onsenui.js",
  "typings": "js/onsenui.d.ts",
  "files": [
    "bower.json",
    "css",
    "css-components-src",
    "js",
    "esm"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OnsenUI/OnsenUI"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OnsenUI/OnsenUI/issues"
  },
  "keywords": [
    "phonegap",
    "cordova",
    "web-components",
    "react",
    "components",
    "angular",
    "vue",
    "hybrid",
    "app",
    "ui",
    "mobile",
    "onsen",
    "onsenui"
  ],
  "homepage": "https://onsen.io/",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "iOS >= 8.0",
              "Android >= 4.4",
              "Chrome >= 30",
              "Safari >= 9",
              "Firefox ESR",
              "Opera 12.1"
            ]
          }
        }
      ],
      "stage-3"
    ]
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-plugin-angularjs-annotate": "0.10.0",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-3": "6.24.1",
    "browser-sync": "2.26.7",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-spies": "1.0.0",
    "chalk": "2.4.2",
    "dateformat": "3.0.3",
    "eslint": "6.6.0",
    "event-stream": "4.0.1",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-babel": "8.0.0",
    "gulp-clean": "0.4.0",
    "gulp-concat": "2.6.1",
    "gulp-cssmin": "0.2.0",
    "gulp-hub": "4.2.0",
    "gulp-if": "3.0.0",
    "gulp-insert": "0.5.0",
    "gulp-load-plugins": "2.0.1",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-util": "3.0.8",
    "gulp-zip": "5.0.1",
    "jsonschema": "1.2.4",
    "karma": "4.4.1",
    "karma-chai": "0.1.0",
    "karma-chai-as-promised": "0.1.2",
    "karma-chai-spies": "0.1.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.1",
    "karma-junit-reporter": "2.0.1",
    "karma-mocha": "1.3.0",
    "karma-rollup-preprocessor": "7.0.2",
    "karma-safari-launcher": "1.0.0",
    "karma-sauce-launcher": "2.0.2",
    "merge-stream": "2.0.0",
    "mkpath": "1.0.0",
    "mocha": "6.2.2",
    "rollup": "1.26.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-execute": "1.1.1",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-progress": "1.1.1",
    "rollup-plugin-string": "3.0.0",
    "rollup-plugin-visualizer": "0.9.2",
    "typescript": "2.9.2",
    "uglify-js": "3.6.4",
    "wcdoc": "0.1.19",
    "wdio-jasmine-framework": "0.3.8",
    "webdriverio": "4.14.4"
  },
  "dependencies": {
    "@onsenui/custom-elements": "1.0.0",
